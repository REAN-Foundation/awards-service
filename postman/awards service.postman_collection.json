{
	"info": {
		"_postman_id": "fa675033-2ef6-4089-8644-ecae912f5a5d",
		"name": "awards service",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
		"_exporter_id": "5510218"
	},
	"item": [
		{
			"name": "Admin Login with password",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"try {\r",
							"    var jsonRes = pm.response.json();\r",
							"    pm.environment.set(\"ADMIN_JWT\", jsonRes.Data.AccessToken);\r",
							"}\r",
							"catch (error) {\r",
							"    console.log(error.message);\r",
							"}\r",
							"\r",
							"pm.test(\"Request is successfull\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"    var jsonRes = pm.response.json();\r",
							"    pm.expect(jsonRes.Status).to.eql('success');\r",
							"});\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "x-api-key",
						"value": "{{X_API_KEY}}",
						"type": "text"
					},
					{
						"key": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"UserName\": \"admin\",\r\n    \"Password\": \"{{ADMIN_PASSWORD}}\"\r\n}\r\n",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{BASE_URL}}/users/login-password",
					"host": [
						"{{BASE_URL}}"
					],
					"path": [
						"users",
						"login-password"
					]
				}
			},
			"response": []
		}
	]
}